programa
{
	
	funcao inicio()
	{
		cadeia nome, sobrenome
		
		escreva("Qual o seu nome? ")
		leia(nome)
		escreva("E sobrenome? ")
		leia(sobrenome)
		escreva("\nOlá ", nome, " ", sobrenome, " seja bem-vindo(a) ao universo da programação\n")
	}
}
